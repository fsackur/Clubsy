@model IPagedList<ClubViewModel>

<div id="clubList">
    
    @foreach (var item in Model)
    {
        <div class="club">
            @Html.Partial("_Club", item)
        </div>
    }

    <div class="pagedList" data-clubsy-target="#clubList">
        @Html.PagedListPager(Model,
                             page => Url.Action("Index", new { page }),
                             PagedListRenderOptions.MinimalWithItemCountText)
    </div>
</div>