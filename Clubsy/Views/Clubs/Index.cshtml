@model IEnumerable<Clubsy.Models.ClubViewModel>

@{
    ViewBag.Title = "Clubs";
}

<h2>Clubs</h2>
<p>
    @Html.ActionLink("Start a new club", "Create")
</p>

<form method="get">
    <input type="search" name="searchTerm" />
    <input type="submit" value="Search" />
</form>

@foreach (var item in Model)
{
    <div class="club">
        @Html.Partial("_Club", item)
    </div>
}

